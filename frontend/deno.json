{
  "tasks": {
    "dev": "deno run -A --node-modules-dir npm:vite",
    "build": "deno run -A --node-modules-dir npm:vite build --mode development",
    "build_production": "deno run -A --node-modules-dir npm:vite build --mode production",
    "preview": "deno run -A --node-modules-dir npm:vite preview"
  },
  "imports": {
    "@googlemaps/markerclusterer": "npm:@googlemaps/markerclusterer@^2.6.2",
    "@tailwindcss/vite": "npm:@tailwindcss/vite@^4.1.12",
    "@types/react": "npm:@types/react@^19.1.10",
    "@types/react-dom": "npm:@types/react-dom@^19.1.7",
    "@vis.gl/react-google-maps": "npm:@vis.gl/react-google-maps@^1.5.5",
    "@vitejs/plugin-react": "npm:@vitejs/plugin-react@^5.0.1",
    "react": "npm:react@^19.1.1",
    "react-dom": "npm:react-dom@^19.1.1",
    "react-google-maps": "npm:react-google-maps@^9.4.5",
    "react-svg": "npm:react-svg@^16.3.0",
    "tailwindcss": "npm:tailwindcss@^4.1.12",
    "tailwindcss-animated": "npm:tailwindcss-animated@^2.0.0",
    "vite": "npm:vite@^7.1.3"
  },
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "dom.asynciterable",
      "deno.ns"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "jsxImportSourceTypes": "@types/react"
  }
}